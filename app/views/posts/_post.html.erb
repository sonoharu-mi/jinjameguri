<thead>
  <tr>
    <th>
      <%= link_to post_path(post.id) do %>
        <%= image_tag post.get_image(200,200) %>
      <% end %>
    </th>
    <th class="text-nowrap"><%= post.shirine_name %></th>
  </tr>
  <tr>
    <th><%= post.address %></th>
  </tr>
  <tr><th><%= post.body %></th></tr>
  <tr>
    <th><%= post.parking_i18n %></th>
    <th><%= post.shirine_stamp_i18n %></th>
  </tr>
  <tr>
    <th><%= link_to "#{post.post_comments.count} コメント", post_path(post.id) %></th>
  </tr>
</thead>

